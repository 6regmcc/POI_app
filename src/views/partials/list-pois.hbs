{{#each pois}}
  <div class="box box-link-hover-shadow">
    <h2 class="title">
      {{name}}
      {{category}}
      {{test}}
      <a href="/dashboard/poi/{{_id}}" class="button">
        <span class="icon is-small">
          <i class="fas fa-folder-open"></i>
        </span>
      </a>
      <a href="/dashboard/delete_poi/{{_id}}" class="button">
        <i class="fas fa-trash"></i>
      </a>

    </h2>
  </div>
{{/each}}